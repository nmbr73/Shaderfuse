  #define swizw(V) (V).zw
  #define swixyx(V) (V).xyx
  #define swiyzx(V) (V).yzx
  #define swixyxy(V) (V).xyxy

#else

  #define swixy(V) to_float2((V).x,(V).y)
//#define swixx(V) to_float2((V).x,(V).x)
  #define swiyx(V) to_float2((V).y,(V).x)
//#define swiyy(V) to_float2((V).y,(V).y)
